{
  "name": "test-consensys",
  "version": "0.0.1",
  "description": "Backend take-home exercise at Consensys",
  "author": "notsag-dev",
  "scripts": {
    "test": "jest --silent src",
    "start": "RUN_PROCESS=1 node dist/index.js",
    "build": "tsc -p .",
    "migrate": "knex migrate:latest --knexfile src/knexfile.ts",
    "add-migration": "knex migrate:make --knexfile src/knexfile.ts",
    "rollback": "knex migrate:rollback --knexfile src/knexfile.ts",
    "dev": "nodemon src/index.ts"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.1",
    "pg": "^8.5.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.2",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^14.14.21",
    "@types/uuid": "^8.3.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^27.0.6",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
